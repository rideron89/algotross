  "version": "0.0.0",
  "description": "Begin basic Svelte app + API",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src --fix",
    "start": "cross-env NODE_ENV=testing ARC_LOCAL=1 npx sandbox",
